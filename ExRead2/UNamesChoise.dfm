      'FROM FLOWER_NAMES a, FLOWER_NAME_TRANSLATIONS b '
      
        'where a.ID_DEPARTMENTS = :ID_DEPARTMENTS and a.fn_id = b.fn_id a' +
        'nd a.FN_ID = :FN_ID'
      'order by F_NAME_RU')
